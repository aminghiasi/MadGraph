set group_subprocesses Auto
set ignore_six_quark_processes False
set loop_optimized_output True
set gauge unitary
set complex_mass_scheme False
import model sm
define p = g u c d s u~ c~ d~ s~
define j = g u c d s u~ c~ d~ s~
define l+ = e+ mu+
define l- = e- mu-
define lep = l+ l-
define vl = ve vm
define vl~ = ve~ vm~
define neut = vl vl~
define V = w+ w-

import model NMSSMHET --modelname
generate p p  > V h01 , V > lep neut, (h01 > a01 a01, a01 > b b~, a01 > b b~)
generate p p  > V h01 j, V > lep neut, (h01 > a01 a01, a01 > b b~, a01 > b b~)
generate p p  > V h01 j j, V > lep neut, (h01 > a01 a01, a01 > b b~, a01 > b b~)


output ggh01_M125_Toa01a01_M20_Tobbbb -nojpeg
